[DEFAULT]
banaction = ufw-subnet

[sshd]
enabled = true

[http-auth]
enabled = true
port = http,https
filter = http-auth
logpath = /var/log/project-prefix/auth.log
maxretry = 5
bantime = 60
findtime = 10
ignoreip = 127.0.0.1/8

[http-access]
enabled = true
port = http,https
filter = http-access
logpath = /var/log/project-prefix/access.log
maxretry = 20
bantime = 10
findtime = 1
ignoreip = 127.0.0.1/8
